@article{ WOS:001367794400001,
Author = {Feng, Danyi and Hicks, Andrea},
Title = {Potential of single-use CO2-based bioplastic water bottle
   adoption using agent-based modeling},
Journal = {ENVIRONMENTAL RESEARCH COMMUNICATIONS},
Year = {2024},
Volume = {6},
Number = {11},
Month = {NOV 1},
Abstract = {Single-use plastic water bottles are ubiquitous in daily life. Most
   commonly these bottles are made from petroleum-based plastics, in
   particular polyethylene terephthalate (PET). However, conventional
   plastic water bottles result in substantial environmental pollution and
   plastic waste. Innovative technologies that utilize captured carbon as
   raw materials for producing bioplastic water bottles offer the potential
   to mitigate at least some environmental impacts. Understanding the
   factors that influence consumer adoption of sustainable alternatives is
   essential. This study utilizes Agent Based Modeling (ABM)to investigate
   the adoption dynamics of bioplastic water bottles, analyzing the
   influence of individual behaviors and social interactions within
   consumer networks. The findings demonstrate that the cost is a
   significant factor influencing the adoption rate, while environmental
   benefits motivate some consumers to select bioplastic water bottles over
   conventional ones. Moreover, the results emphasize the importance of
   social networks, indicating that close connections among consumers lead
   to quicker and more widespread adoption due to peer influence},
