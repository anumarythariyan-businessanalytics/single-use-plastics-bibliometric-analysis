@article{ WOS:001289784800001,
Author = {Hendrickson, Thomas P. and Bose, Baishakhi and Vora, Nemi and
   Huntington, Tyler and Nordahl, Sarah L. and Helms, Brett A. and Scown,
   Corinne D.},
Title = {Paths to circularity for plastics in the United States},
Journal = {ONE EARTH},
Year = {2024},
Volume = {7},
Number = {3},
Month = {MAR 15},
Abstract = {In 2019, the United States consumed over 57 million metric tons (MMT) of
   plastic with less than 7\% recovered for reuse. This study provides an
   updated material flow analysis at national and regional scales for all
   durable and single-use plastics in the United States. From this material
   flow analysis, we develop a series of alternative future national
   plastic flow scenarios that envision a scale-up of recycling
   technologies, incorporating technical limitations and sorting
   infrastructure constraints. The results suggest that a maximum of 68\%
   (24 MMT) of plastic waste could be diverted from landfills by scaling up
   existing commercial recycling technologies. Based on the current
   technological landscape, reaching near-zero waste is only possible if
   processes that are operating at pilot and laboratory scales can be
   effectively scaled and coupled with improved sorting infrastructure.
   Through these scenarios with increased recycling, the availability of
   postconsumer resin stocks could increase by 22-43 MMT.},
