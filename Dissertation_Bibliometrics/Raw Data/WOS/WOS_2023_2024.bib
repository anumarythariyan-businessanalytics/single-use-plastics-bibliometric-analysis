@article{ WOS:001163570300001,
Author = {Chen, Yezhong and Pan, Kaibo and Mai, Kaijin and Jiao, Jian and Zeng,
   Xiangbin and Fu, Qiang and Li, Jianjun},
Title = {Synthesis, Properties, and Degradation of Poly(butylene
   succinate-co-sebacicate-co-salicylicate) Copolyesters},
