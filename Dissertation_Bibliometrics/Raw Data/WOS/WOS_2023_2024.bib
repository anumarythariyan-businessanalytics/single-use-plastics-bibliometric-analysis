@article{ WOS:001115566900001,
Author = {Nishijima, Asako and Nakatani, Jun},
Title = {Survey and analysis on the use and disposal of plastic shopping bags
   before and after the introduction of charges},
Journal = {JOURNAL OF MATERIAL CYCLES AND WASTE MANAGEMENT},
Year = {2024},
Volume = {26},
Number = {2},
Pages = {741-754},
Month = {MAR},
Abstract = {Japan implemented mandatory charging for single-use plastic shopping
   bags in 2020. In this study, we verified the policy effect and its
   persistence by analyzing the changes in the number of plastic shopping
   bags taken and discarded by consumers before and after the charging came
   into effect. Online surveys were conducted in 2016, 2021, 2022, and 2023
   for consumers residing in municipalities in Tokyo. The results showed
   that the number of plastic shopping bags taken decreased to
   approximately 30\% after the charging was implemented, from 5.8 bags per
   person per week in 2016 to 1.7-1.9 in 2021-2023, and the number of bags
   discarded decreased from 6.2 to 3.0-3.4. The number of bags taken in
   2021-2023 after the charging was implemented did not change
   significantly. Regarding the relationship with other waste policies, in
   municipalities that have introduced designated trash bags, the number of
   plastic shopping bags taken and discarded was fewer than in other
   municipalities even before the charging was introduced, by removing the
   option of reusing plastic bags as trash bags. This result indicates
   that, when introducing such policies, the relationship with existing
   waste policies must be analyzed to predict their effect.},
