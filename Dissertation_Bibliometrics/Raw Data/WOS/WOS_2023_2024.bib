@article{ WOS:001220760500001,
Author = {Gomez-Dopazo, Gabriela B. and Nieves, Renis J. Agosto and Rivera,
   Rolando L. Albarracin and Morera, Shaneily M. Colon and Nazario, Daniel
   Rivera and Ramos, Idalia and Dmochowski, Ivan J. and Lee, Daeyeon and
