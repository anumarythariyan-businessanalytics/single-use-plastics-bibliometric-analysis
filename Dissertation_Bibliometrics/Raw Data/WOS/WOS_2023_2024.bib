@article{ WOS:001079484400001,
Author = {Nandan, H. Vishnu and Anish, R. and Jayaprasad, G. and Sunny, Subin and
   Jomon, Jackson Scaria and Shibin, R. and Sajin, J. B.},
Title = {Experimental investigations on the mechanical properties and thermal
   analysis of coir-paper hybrid composite carry bag material},
Journal = {MATERIALS LETTERS},
Year = {2023},
Volume = {353},
Month = {DEC 15},
Abstract = {Increasing concern about environmental pollution has led researchers to
   find sustainable alternatives to synthetic counterparts. With the
   packaging industry accounting for 42 percent of plastics entering our
   marine systems, governments have also passed legislation banning
   single-use plastics. This paper essays the fabrication and experimental
   validation of hybrid bio composite with coir and cotton fiber
   reinforcement dispersed in paper matrix. Specimen with varying
   concentration of coir and different fiber length was validated against
   150 GSM standard packaging paper material. The specimen constituted by
   20 wt\% coir was observed to have better tensile strength of 2.678
   N/mm2, Izod higher impact value of 0.15 J compared to the standard
   sample which had tensile strength of 1.875 N/mm2 and lower Izod impact
   value of 0.10 J. Increasing coir content led to 142.4\% higher water
   absorption and was not appreciable for making carry bags. The thermal
   stability characteristics of composite specimens were comparable to
   conventional packaging materials.},
