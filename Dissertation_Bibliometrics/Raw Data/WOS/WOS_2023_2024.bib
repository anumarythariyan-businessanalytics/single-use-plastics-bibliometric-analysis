@inproceedings{ WOS:001450208000067,
Author = {Tinoco, Joao and Lima, Claudia and Mendonca, Rui},
Editor = {Martins, N and Brandao, D},
Title = {Intervention of Product Design in E-Commerce: Contribution to a
   Sustainable Proposal in Supermarket Delivery Services},
