@article{ WOS:001245576100001,
Author = {Suchat, Sunisa and Boonrasri, Siwarote},
Title = {Advancement of an Environmentally Friendly and Innovative Sustainable
   Rubber Wrap Film with Superior Sealing Properties},
Journal = {POLYMERS},
