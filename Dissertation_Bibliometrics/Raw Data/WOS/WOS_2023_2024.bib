@article{ WOS:001324506700001,
Author = {Shorey, Rohan and Ataeian, Parinaz and Mekonnen, Tizazu H.},
Title = {Effect of acetylation of kraft lignin on the blend compatibility with
   cellulose acetate and the resulting physicomechanical properties},
Journal = {CELLULOSE},
Year = {2024},
