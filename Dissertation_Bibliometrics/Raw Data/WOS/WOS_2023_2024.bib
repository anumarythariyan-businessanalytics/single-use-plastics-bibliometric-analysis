Author = {Antalfy, Attila and Berman, Kyle and Everitt, Claire and Alten, Rieke
   and Latymer, Mark and Godfrey, Charles M.},
Title = {The Adherence and Outcomes Benefits of Using a Connected, Reusable
   Auto-Injector for Self-Injecting Biologics: A Narrative Review},
Journal = {ADVANCES IN THERAPY},
Year = {2023},
Volume = {40},
Number = {11},
