@article{ WOS:001433933600001,
Author = {Bharadwaj, Bishal and Ujjwal, Kc and Subedi, Mukti Nath},
Title = {Optimal inspection (monitoring) and penalty (fine) for plastic bag ban
   compliance in Nepal},
