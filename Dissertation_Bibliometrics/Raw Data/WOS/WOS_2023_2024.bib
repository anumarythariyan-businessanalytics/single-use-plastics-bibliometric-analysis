@article{ WOS:001366067000001,
Author = {Antoniadou, Maria},
Title = {Integrating Lean Management and Circular Economy for Sustainable
   Dentistry},
Journal = {SUSTAINABILITY},
Year = {2024},
Volume = {16},
Number = {22},
Month = {NOV},
Abstract = {The increasing focus on environmental sustainability has prompted the
   healthcare sector, including dentistry, to reconsider waste management
   practices. Dental offices generate various waste streams from outdated
   technologies, including single-use plastics, hazardous materials, and
   digital waste. This scoping review explores how integrating circular
   economy principles with lean management practices can enhance
   sustainability and operational efficiency in dental clinics. Using the
   PRISMA methodology, the review identifies 30 articles that passed the
   Cochrane Risk of Bias Tool and the Critical Appraisal Skills Program
   (CASP) checklist from 2000. Circular economy principles focus on
   reducing waste and maximizing resource reuse, while lean management
   seeks to improve efficiency and minimize process waste. Together, these
   approaches can significantly reduce resource consumption and enhance
   recycling efforts in dental practices. Despite barriers such as high
   technology costs, regulatory constraints, and limited recycling
   infrastructure, case studies highlight successful implementation in
   healthcare settings, demonstrating improved environmental and
   operational outcomes. Collaboration between dental professionals,
   policymakers, and industry leaders is essential for promoting the
   adoption of these sustainable practices.},
