Author = {Wang, Wanli and Themelis, Nickolas J. and Sun, Kai and Bourtsalas,
   Athanasios C. and Huang, Qunxing and Zhang, Yunhe and Wu, Zhaohui},
Title = {Current influence of China's ban on plastic waste imports},
