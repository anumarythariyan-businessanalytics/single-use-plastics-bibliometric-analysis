@article{ WOS:000494952200057,
Author = {Krishnamurthy, Akshaya and Amritkumar, Pavithra},
Title = {Synthesis and characterization of eco-friendly bioplastic from low-cost
   plant resources},
Journal = {SN APPLIED SCIENCES},
Year = {2019},
Volume = {1},
Number = {11},
Month = {NOV},
Abstract = {Alarming rate of environmental pollution caused by single-use plastics
   has necessitated the search for developing sustainable yet,
   cost-effective alternatives. This study aimed to develop bioplastic
   films using combination of jackfruit waste flour and sago as a low-cost
   starch source. Three composite bioplastic films were developed using
   starch mix with lemon extract and water but with different
   cross-linkers: (1) PV (polylactic acid and crude palm oil), (2) AV
   (glycerol and crude palm oil), (3) PAV (polylactic acid, glycerol and
   crude palm oil). The formulated films' physical, chemical, mechanical
   and biological parameters were characterized. PV film showed the best
   tensile strength (TS) of 5.24 MPa and least elongation at break (EAB) of
   3.49\%, while PAV film had the least TS of 0.78 MPa with a maximum EAB
   (73.38\%). FTIR analysis of films showed peak at 1736.3-1743.3 cm(-1)
   indicating strong interaction of triglycerides (C=O stretching). In
   thermogravimetric analysis (TGA), all the films displayed gradual
   desorption occurring between 150 and 220 degrees C where the volatile
   compounds started to evaporate and around 300 degrees C the lower
   molecular weight compounds like plasticizer, cross-linker and other
   additives started to release out; pyrolysis of carbonated compounds
   occurred above 500 degrees C. PAV film displayed best thermal stability
   with half decomposition at 330 degrees C. Biodegradability of films was
   demonstrated through soil burial method and culturing of pure cultures
   of microorganisms in minimal media by providing the film as carbon
   source. Bioplastic films developed in this study using low-cost plant
   starch sources have displayed properties with potential application in
   food packaging industry.},
