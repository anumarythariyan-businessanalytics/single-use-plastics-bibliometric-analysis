@article{ WOS:000569614800053,
Author = {Rhein, Sebastian and Schmid, Marc},
Title = {Consumers' awareness of plastic packaging: More than just environmental
   concerns},
Journal = {RESOURCES CONSERVATION AND RECYCLING},
Year = {2020},
Volume = {162},
Month = {NOV},
Abstract = {Plastic pollution is considered to be among the greatest challenges the
   world faces. The consumption and often uncontrolled disposal of
   single-use plastic packaging is regarded as one of the main
   environmental hazards. Whereas the role of companies and politics in
   addressing this problem has been discussed intensively, little focus has
   been placed on the role of consumers. This paper investigates consumers'
   awareness of plastic packaging by following an inductive and more
   differentiated approach. Results show that consumers associate plastic
   packaging with far more than environmental problems. Five different
   types of consumers' awareness can be identified. Results help to gain a
   broader understanding of consumers and thus help to develop more
   effective strategies that empower consumers to use plastic in a more
   sustainable way.},
