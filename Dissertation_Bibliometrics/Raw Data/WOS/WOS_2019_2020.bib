@article{ WOS:000555910000022,
Author = {Gonnade, E. M. and Joshi, V, S. and Kulkarni, P. B. and Tiwari, M. S.},
Title = {Reuse of Synthetic Plastic Fibres and Plastic Granules in Concrete},
