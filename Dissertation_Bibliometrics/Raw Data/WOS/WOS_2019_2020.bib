
@article{ WOS:000457814500004,
Author = {Jahani, Akram and Dehdari, Tahereh and Farzadkia, Mandi and Mansourian,
   Morteza},
Title = {Iranian experiences in terms of consumption of disposable single-use
   plastics: Introduction to theoretical variables for developing
   environmental health promotion efforts},
Journal = {ENVIRONMENTAL TOXICOLOGY AND PHARMACOLOGY},
Year = {2019},
Volume = {65},
Pages = {18-22},
Month = {JAN},
Abstract = {In recent years, the consumption of disposable plastic items such as
   single-use plastic bags, plates, cutlery and so on has rapidly increased
   in Iran. Present study explored the Iranians' experiences in terms of
   the consumption of disposable single-use plastics. A qualitative content
   analysis study was conducted in Delfan, Lorestan, Iran in 2017. Thirty
   semi-structured in-depth interviews were done with citizens who resided
   in Delfan. Participants were asked about their experiences of single-use
   plastics consumption. After the first interview, continuous analysis of
   data began and continued up to data saturation. Three main themes
   including perceived barriers for less consumption of disposable
   single-use plastics, decisional balance and structural factors emerged
   as the Iranians' experiences in terms of the consumption of disposable
   single-use plastics. Health promotion practitioners should increase
   public awareness about the harmful outcomes of high consumption of
   disposable single use plastic items through mass media. In addition,
   they should convince policymakers to enact laws for increasing the
   production of recyclable single-use items.},
