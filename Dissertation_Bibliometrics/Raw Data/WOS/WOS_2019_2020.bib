@article{ WOS:000515302900001,
Author = {Jennings, Freddie J. and Suzuki, Valeria P. and Hubbard, Alexis},
Title = {Social Media and Democracy: Fostering Political Deliberation and
   Participation},
Journal = {WESTERN JOURNAL OF COMMUNICATION},
Year = {2021},
Volume = {85},
Number = {2},
Pages = {147-167},
Month = {MAR 15},
Abstract = {A democracy extends beyond partisan politics. The current study examines
   how a social media video about the environment can spark political
   deliberation and participation. Drawing from normative democratic
   theory, an experimental design was employed to test the effect of the
   video in engaging citizens in the political process and fostering
   interpersonal conversations, both online and face-to-face. At the core
   of these normative democratic outcomes is information efficacy, as
   individuals are moved to discuss the issue and act to protect the
   environment from the threat of single-use plastics. The results reveal
   that social media have the potential to have a positive democratic
   influence.},
