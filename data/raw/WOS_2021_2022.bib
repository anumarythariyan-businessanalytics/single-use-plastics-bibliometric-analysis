@article{ WOS:000620260400004,
Author = {Uhrin, V, Amy and Laverty, Amanda L. and Fox, Demi and Herring, Carlie
   E. and Kehoe, Christy and Latshaw, Sarah and Lippiatt, Sherry M.},
Title = {Walking the talk: The responsibility of the scientific community for
   mitigating conference-generated waste},
Journal = {MARINE POLLUTION BULLETIN},
Year = {2021},
Volume = {163},
Month = {FEB},
Abstract = {An estimated 19-23 million metric tons of global plastic waste
   reportedly entered aquatic environments in 2016 with mounting evidence
   that plastic marine debris causes ecological effects across all levels
   of biological organization in aquatic systems. Scientific conferences
   generate opportunities for waste through food and beverage services,
   giveaways, marketing and registration materials, poster and trade
   exhibits, attendee travel, lodging services, and local transportation.
   Zero waste measures instituted at the Sixth International Marine Debris
   Conference resulted in the avoidance of 76,300 single-use items. Zero
   waste is a process defined by a spectrum of actions ranging from no
   reduction whatsoever to generation of absolutely no waste. Achieving
   100\% zero waste is very difficult. Deciding where on the spectrum you
   wish to land and being comfortable with that target is paramount for
   event planning. Planning for reduced waste takes time, funding, and
   determination, but environmentally-themed organizations have a
   responsibility to lead by example.},
