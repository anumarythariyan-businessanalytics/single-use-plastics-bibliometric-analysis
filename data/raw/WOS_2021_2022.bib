
@article{ WOS:000640367400001,
Author = {Tan, Wenbing and Cui, Dongyu and Xi, Beidou},
Title = {Moving policy and regulation forward for single-use plastic alternatives},
Journal = {FRONTIERS OF ENVIRONMENTAL SCIENCE \& ENGINEERING},
Year = {2021},
Volume = {15},
Number = {3},
Month = {APR 12},
Abstract = {Single-use plastics are often used once or cannot be reused for extended
   periods. They are widely consumed with the rapid development of social
   economy. The waste generated by single-use plastics threatens ecosystem
   health by entering the environment and ultimately restricts sustainable
   human development. The innovation of sustainable and environmentally
   friendly single-use plastic alternative materials and the joint
   participation of governments, enterprises and the public are promising
   technologies and management approaches that can solve the problem of
   single-use plastics wastes. The development of single-use plastic
   alternative products can be promoted fundamentally only by improving
   relevant legislation and standards, providing differentiated industrial
   policies, encouraging scientific and technological innovation and
   expanding public participation.},
