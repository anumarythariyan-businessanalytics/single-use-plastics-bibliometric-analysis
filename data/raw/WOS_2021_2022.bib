@article{ WOS:000791336000001,
Author = {Cherrier, Helene and Ture, Meltem},
Title = {Blame work and the scapegoating mechanism in market status-quo},
Journal = {JOURNAL OF BUSINESS RESEARCH},
Year = {2022},
Volume = {144},
Pages = {1207-1217},
Month = {MAY},
Abstract = {ABSTR A C T This study explores the tenacity of the plastics market and
   reveals blame work to explain market status quo. Anti-plastic advocates
   accuse corporations and regulators of inefficient measures deployed to
   control the omnipres-ence, hazardous effects, and indestructability of
   plastic. Regulators focus on single-use plastic and recycling to
   mitigate the omnipresence of plastic and delegate responsibility to
   other actors for its hazardous effects and indestructability. Plastic
   producers and distributors avoid blame by developing alternatives to
   single-use plastic. Consumers locally and sporadically blame policy
   makers and corporations for plastic problems but lack resources and
   social organization to effect broad changes in the market. From all
   these dynamics of blame emerges a scapegoat: consumers accepting
   responsibility for plastic problems. Dynamics of blame help maintain
   boundaries of power and interest and contribute to consumer
   responsibilization, reinforce power imbalances, hinder collaboration
   among actors with different interests in plastic, and, as such, help
   maintain market status quo.},
