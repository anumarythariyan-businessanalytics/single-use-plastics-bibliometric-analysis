@article{ WOS:000608181500008,
Author = {Ali, Sharafat and Ahmed, Waqas and Solangi, Yasir Ahmed and Sharif,
   Chaudhry Imran and Zarei, Nasibeh},
Title = {Strategic analysis of single-use plastic ban policy for environmental
   sustainability: the case of Pakistan},
