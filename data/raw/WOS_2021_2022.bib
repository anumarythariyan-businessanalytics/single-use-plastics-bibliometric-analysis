@article{ WOS:000865701500002,
Author = {Sadasivan, Eshan and Manoharan, Kapil and Das, Mainak and Bhattacharya,
   Shantanu},
Title = {Rural Employment Through Product Development: Entrepreneurial Framework
   for Grassroot Level Innovations},
Journal = {JOURNAL OF THE KNOWLEDGE ECONOMY},
