@article{ WOS:000851681400001,
Author = {Mugobo, Victor Virimai and Ntuli, Herbert},
Title = {Consumer Preference for Attributes of Single-Use and Multi-Use Plastic
   Shopping Bags in Cape Town: A Choice Experiment Approach},
Journal = {SUSTAINABILITY},
Year = {2022},
