@article{ WOS:000893821900038,
Author = {Dailin, Daniel Joe and Rithwan, Fahim and Hisham, Adibah Mohd and Rasid,
   Zaitul Iffa Abd and Azelee, Nur Izyan Wan and Sapawe, Norzahir and
   Chuah, Lai Fatt and Yusof, Abdul Halim Mohd and El Enshasy, Hesham},
Title = {A Review on current status of plastic waste Biodegradation using
   microbial approach},
Journal = {BIOSCIENCE RESEARCH},
