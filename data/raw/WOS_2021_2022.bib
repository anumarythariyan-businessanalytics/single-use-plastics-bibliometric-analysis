@article{ WOS:000641381900001,
Author = {Rodriguez, L. Joana and Ospina, Sebastian and Ribeiro, Ines and Pecas,
   Paulo and Orrego, Carlos E.},
Title = {Banana fibre-biocomposite applied to bottle lid case-life-cycle
   engineering model for material selection},
