@article{ WOS:000677394600009,
Author = {Tyagi, Preeti and Salem, Khandoker Samaher and Hubbe, Martin A. and Pal,
   Lokendra},
Title = {Advances in barrier coatings and film technologies for achieving
   sustainable packaging of food products-A review},
Journal = {TRENDS IN FOOD SCIENCE \& TECHNOLOGY},
