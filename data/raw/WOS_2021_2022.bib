@article{ WOS:000819809300002,
Author = {Knoblauch, Doris and Mederake, Linda},
Title = {Government policies combatting plastic pollution},
Journal = {CURRENT OPINION IN TOXICOLOGY},
Year = {2021},
Volume = {28},
Pages = {87-96},
Month = {DEC},
Abstract = {This review analyzes the subject focus of 45 articles (published
   2019-2021), dealing with government action to regulate plastic
   pollution. Policies described in the articles and introduced between
   2016 and 2021 are clustered in four categories: production - consumption
   - disposal - circular approaches. Our results show that most published
   articles deal with bans on single-use plastic (SUP) items in one or more
   countries. While SUP bags are still the most regulated items, there is a
   growing number of regulations on other SUP items, such as Styrofoam
   products or microbeads. While policies and research focus heavily on the
   consumption phase, the production phase is not only under-regulated, but
   also under-researched.},
