Title = {Metabolic Consequences of Developmental Exposure to Polystyrene
   Nanoplastics, the Flame Retardant BDE-47 and Their Combination in
   Zebrafish},
Journal = {FRONTIERS IN PHARMACOLOGY},
Year = {2022},
Volume = {13},
