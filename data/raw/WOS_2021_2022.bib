@article{ WOS:000702808300008,
Author = {Espana Keller, Juliana},
Title = {GRETA//A PLASTIC POEM: An Integrated Approach to the Vibrant Matter of
   Voice, Deep Listening, and Somatic Movement in Sonic Performance Art as
   Plastic Activism},
