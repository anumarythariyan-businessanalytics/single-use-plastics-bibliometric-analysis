@article{ WOS:000865412000015,
Author = {Dioses-Salinas, Diana Carolina and Pizarro-Ortega, Carlos Ivan and
   Dobaradaran, Sina and Ben-Haddad, Mohamed and De-la-Torre, Gabriel
   Enrique},
Title = {Face masks invading protected areas: Risks and recommendations},
Journal = {SCIENCE OF THE TOTAL ENVIRONMENT},
Year = {2022},
Volume = {847},
Month = {NOV 15},
Abstract = {Among the indirect environmental impacts generated by the global
   COVID-19 pandemic, contamination with personal protective equipment
   (PPE), like face masks, may be one of the most relevant ones. PPE has
   been found in multiple aquatic, marine, and terrestrial environments,
   including places of absolute relevancy to biodiversity conservation,
   such as protected areas (PM). Here, a brief report of the presence of
   PPE in six PM of Peru is presented. PPE pollution in PM consisted mainly
   of single-use and reusable face masks, as well as plastics associated
   with PM, such as KN95 respirator wrappings. The mean PPE density was
   estimated as 1.32 x 10(-3) PPE/m(2). FTIR spectroscopy confirmed that
   face masks and wrappers mainly consisted of polypropylene and
   polyethylene, two of the most commonly available synthetic polymers. The
   material was poorly degraded according to their FTIR spectra, possibly
   suggesting that they were discarded recently. The recent ban on
   single-use plastic in Peruvian PM is regarded as a great step forward
   toward the efforts made to preserve these invaluable places. However,
   these measures seemed insufficient to prevent PPE and other types of
   litter from contaminating areas of ecological importance. Considering
   the current scenario, several recommendations were proposed to be
   implemented in PM in order to prevent PPE from becoming a new plastic
   issue to tackle. These recommendations are expected to also serve for
   future events where the use of single-use plastics becomes inevitable,
   like global pandemics.},
Publisher = {ELSEVIER},
Address = {RADARWEG 29, 1043 NX AMSTERDAM, NETHERLANDS},
Type = {Article},
