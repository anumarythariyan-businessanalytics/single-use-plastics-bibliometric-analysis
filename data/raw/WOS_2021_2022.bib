@article{ WOS:000987733100001,
Author = {Glogowska, Margaret and Croxson, Caroline and Butler, Christopher and
   Hayward, Gail},
Title = {Experiences of urine collection devices during suspected urinary tract
   infections: a qualitative study in primary care},
