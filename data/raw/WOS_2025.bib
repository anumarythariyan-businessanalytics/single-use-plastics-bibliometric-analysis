@article{ WOS:001608333500001,
Author = {Meshram, Bhushan P. and Bornare, Shreya and Gaikwad, Kirtiraj K.},
Title = {Development of smart edible spoons with pH-sensitive halochromic
   properties from kodo millet starch reinforced with Clitoria
   ternatea anthocyanins},
Journal = {INNOVATIVE FOOD SCIENCE \& EMERGING TECHNOLOGIES},
Year = {2025},
Volume = {106},
Month = {DEC},
Abstract = {The increasing environmental burden of single-use plastic cutlery
   necessitates the development of sustainable, functional, and
   biodegradable alternatives to this material. In this study, pH-sensitive
   smart edible spoons were fabricated from kodo millet (Paspalum
   scrobiculatum) starch and xanthan gum (KD/XG) reinforced with Clitoria
   ternatea anthocyanins (butterfly pea extract, BPE). The effect of BPE
   incorporation (3-9 \%) on the structural, functional, and intelligent
   properties of the spoons was systematically investigated. FTIR and XRD
   analyses revealed hydrogen bonding interactions between anthocyanins and
   the KD/XG matrix, accompanied by a reduction in crystallinity. Thermal
   analysis indicated improved stability, with the onset of degradation
   shifting from 257 degrees C (control) to 275 degrees C (9 \% BPE).
   Antioxidant assays showed a dose-dependent increase in DPPH radical
   scavenging activity from 28.6 \% (control) to 45.3 \% (9 \% BPE). The
   spoons exhibited distinct and reversible pH-dependent color transitions
   (violet -> blue -> green, pH 3-10), enabling visual monitoring of food
   freshness. Microbial counts decreased below 5 log CFU/g after 30 days of
   storage, and soil burial tests confirmed complete biodegradation within
   20-30 days. Sensory evaluation identified 6 \% BPE as the optimal
   formulation, balancing mechanical strength, appearance, and consumer
   acceptability. Overall, this work demonstrates the synergistic role of
   millet starch, xanthan gum, and anthocyanins in developing edible,
   pH-responsive spoons that combine durability, antioxidant protection,
   and real-time freshness indication, representing a significant step
   toward intelligent biodegradable food packaging.},
