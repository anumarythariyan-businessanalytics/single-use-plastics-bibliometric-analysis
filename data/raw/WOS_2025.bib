@article{ WOS:001572598100001,
Author = {Sy, Deborah K. and Momas, Chloe and Beguinot, Emmanuelle and Arnold,
   Danielle and van Kalmthout, Danielle and Olefir, Lilia},
Title = {Turning tobacco Extended Producer Responsibility (EPR) into Extended
   Producer Liability (EPL): critical safeguards for the UN Plastics Treaty},
Journal = {TOBACCO CONTROL},
Year = {2025},
Month = {2025 AUG 12},
Abstract = {Extended Producer Responsibility (EPR) is recognised as a valuable tool
   for environmental management of products' end-of-life impacts; it was
   featured in all the draft negotiating text for the future UN Plastics
   Treaty. However, when applied to the tobacco industry, its
   implementation faces significant challenges due to the industry's
   historical manipulation of health policies. This study explores the
   inherent contradictions in using EPR schemes for tobacco products, which
   are designed to make producers `stewards' for the life cycle of their
   products, including end-of-life impacts. The study highlights the
   potential for tobacco companies to exploit these schemes to weaken
   health regulations and greenwash their public image. By examining
   frameworks like the European Union Single-Use Plastics Directive
   alongside the UN Plastics Treaty negotiations, the study stresses the
   need for stringent safeguards to ensure EPR schemes do not serve as
   tools for greenwashing but support health and environmental objectives.
   The study also proposes enhanced regulatory measures, such as redefining
   EPR for tobacco as `Extended Producer Liability' and integrating it with
   WHO Framework Convention on Tobacco Control guidelines.},
Publisher = {BMJ PUBLISHING GROUP},
Address = {BRITISH MED ASSOC HOUSE, TAVISTOCK SQUARE, LONDON WC1H 9JR, ENGLAND},
Type = {Article; Early Access},
Language = {English},
Affiliation = {Sy, DK (Corresponding Author), Global Ctr Good Governance Tobacco Control, Bangkok, Thailand.
   Sy, Deborah K., Global Ctr Good Governance Tobacco Control, Bangkok, Thailand.
   Momas, Chloe; Olefir, Lilia, Smoke Free Partnership, Brussels, Belgium.
   Beguinot, Emmanuelle, CNCT Com Natl Tabagisme, Natl Comm Tobacco Control, Paris, France.
   Arnold, Danielle, Hlth Funds Smoke free Netherlands, Utrecht, Netherlands.
   van Kalmthout, Danielle, Belgian Alliance Smoke Free Soc, Brussels, Belgium.},
