@article{ WOS:001491391300009,
Author = {Berthelsen, Ane Liv and Paijmans, A. J. and Forcada, Jaume and Hoffman,
   Joseph Ivan},
Title = {Sustainability in the laboratory: evaluating the reusability of
   microtitre plates for PCR and fragment detection},
Journal = {ROYAL SOCIETY OPEN SCIENCE},
Year = {2025},
Volume = {12},
Number = {5},
Month = {MAY 21},
Abstract = {Single-use plastics (SUPs) are indispensable in laboratory research, but
   their disposal contributes substantially to environmental pollution.
   Consequently, reusing common SUP items such as microtitre plates
   represents a promising strategy for improving laboratory sustainability.
   However, the key challenge lies in determining whether SUP reuse can be
   implemented without sacrificing data quality. To investigate this, we
   conducted a simple experiment to assess the impact of reusing microtitre
   plates on microsatellite genotyping accuracy. Plates previously used for
   polymerase chain reaction (PCR) and fragment detection were cleaned,
   opting for an environmentally friendly approach using regular soap, and
   then reused. Our results indicate that, while reusing PCR plates
   significantly increases genotyping error rates due to residual DNA
   contamination, detection plates can potentially be reused without
   compromising data quality. Our approach offers laboratories a practical
   and sustainable option for reducing SUP waste and costs while
   maintaining research integrity.},
Publisher = {ROYAL SOC},
Address = {6-9 CARLTON HOUSE TERRACE, LONDON SW1Y 5AG, ENGLAND},
