@article{ WOS:001434995200001,
Author = {Pavlovskyi, Denys and Vorobyova, Victoria},
Title = {A Comprehensive Overview of Chemical Additives in Single-Use Polimeric
   Products: Functionality, Environmental Impact and the Analytical
   Greenness Assessment},
Journal = {WATER AIR AND SOIL POLLUTION},
Year = {2025},
Volume = {236},
Number = {3},
Month = {MAR},
Abstract = {This review presents the first comparative analysis of functional
   chemical additives in various types of single-use plastics (SUPs). These
   plastics, commonly used in industries like packaging, food service, and
   healthcare, contain additives such as stabilizers, colorants, flame
   retardants, and plasticizers to enhance their flexibility, durability,
   and appearance. However, many of these additives are not chemically
   bonded to the polymer matrix and can leach out during the plastic's life
   cycle, posing risks to human health and the environment. The review
   explores the functional characteristics, mechanisms, and benefits of
   these additives, and highlights recent advances in bio-based and
   additive-free alternatives. The study also uses the AGREEMIP tool to
   assess the greenness of the synthesis processes for various SUP
   products, ranking them in the order of environmental safety: disposable
   plastic containers for hedgehogs > disposable plastic dances >
   disposable glasses and plates > drinking straws and sticks > plastic
   bags.},
