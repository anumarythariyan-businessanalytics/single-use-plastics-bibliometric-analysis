@article{ WOS:001625104000001,
Author = {Gell, Peter A. and Soeprobowati, Tri R. and Rahim, Aulia and Wilby,
   Robert L.},
Title = {Cascading Effects of Surface Water Pollution and Land Subsidence in
   Indonesia},
Journal = {COASTAL MANAGEMENT},
Year = {2025},
Month = {2025 NOV 16},
Abstract = {The central Javan city of Semarang has a population of 2 million people
   and annually receives over 2000 mm of rainfall. Intensive development in
   the Semarang River catchment has polluted surface waters precluding
   their use as a potable supply. This has led to the abstraction of
   groundwater as the main potable source, distributed via single use
   plastic bottles. Unsustainable groundwater exploitation has resulted in
   >> 8 cm/yr of subsidence contributing to the inundation of many coastal
   residences, as well as loss of productive land and public
   infrastructure. Hard coastal barriers and the channelization of rivers
   to minimize flooding are among proposed mitigation measures. The
   long-term effectiveness of these actions is limited and over one million
   residents could be at risk of marine inundation by 2050. Thus, an
   adaptation pathways approach to mitigate subsidence is proposed aimed at
   substituting groundwater abstraction through: (1) constructing small
   water supply reservoirs in protected, upper catchment areas; (2)
   capturing and filtering rainwater to provide a decentralized
   alternative, and (3) mitigating pollution to reestablish more plentiful
   surface water supplies. Once the rate of subsidence is eased, mangrove
   restoration and targeted engineering may be capable of reinstating a
   hydrogeological balance and accommodating climate-driven sea level rise.},
