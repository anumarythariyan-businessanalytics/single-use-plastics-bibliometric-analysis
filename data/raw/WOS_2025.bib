@article{ WOS:001601640500001,
Author = {Dordevic, Dani and Danilovic, Bojana and Cvetkovic, Kristina and
   Zelenka, Lena and Kotianova, Dominika and Dordevic, Simona},
Title = {Development of Biodegradable Straws Using Spent Coffee Grounds},
