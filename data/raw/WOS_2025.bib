Title = {Replacing Single-Use Plastics (SUPs) in Coastal Festivals: A Critical
   Evaluation of Bio-Based, Biodegradable and Compostable Tableware,
   Regulatory Policies and Public Awareness},
Journal = {INTERNATIONAL JOURNAL OF ENVIRONMENTAL RESEARCH},
Year = {2025},
Volume = {19},
