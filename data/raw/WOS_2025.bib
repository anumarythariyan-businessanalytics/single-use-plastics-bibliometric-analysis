@article{ WOS:001452778000001,
Author = {Tancu, Ana Maria Cristina and Imre, Marina and Iosif, Laura and Pituru,
   Silviu Mirel and Pantea, Mihaela and Sfeatcu, Ruxandra and Ilinca, Radu
