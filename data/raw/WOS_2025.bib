@article{ WOS:001544653600005,
Author = {Hassan, Leoneeta Rozwen and Selamat, Ali and Nordin, Syarifah Zyurina},
Title = {Prioritizing Criteria for Reducing Single-Use Plastic Waste in
   Foodservice: A Fuzzy Analytic Hierarchy Process (FAHP)-Based Study with
   Key Player Insights},
Journal = {MATEMATIKA},
