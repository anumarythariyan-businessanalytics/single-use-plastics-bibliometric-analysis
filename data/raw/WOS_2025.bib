@article{ WOS:001568735400011,
Author = {Wannawat, Warut and Supakata, Nuta},
Title = {Sustainable single-use plastic management on Sichang Island, Thailand},
Journal = {RESULTS IN ENGINEERING},
Year = {2025},
Volume = {27},
Month = {SEP},
Abstract = {In this study, we investigated plastic waste management on Sichang
   Island, Thailand, with a focus on single-use plastic waste and polymer
   composition analysis. A total of seven single-use plastic categories are
   referenced in this papers according to Thailand's Plan for Plastic Waste
   Management Phase II (2023-2027) that attempts to reduce plastic waste in
   landfills, increase recycling, and decrease plastic waste entering the
   ocean. Plastic waste (49.17\%) is the dominate solid waste material on
   Sichang Island. The results of our investigated demonstrated that
   74.47\% of the waste consisted of single-use plastics, and the top three
   dominant single-use plastic-types were plastic bags, single-layer
   plastic film packaging, and plastic bottles. The identified polymer
   types (i.e., LDPE: 31\%; PET: 20.5\%; and HDPE: 18.5\%) were consistent
   with those commonly found in single-use plastics. This study assessed
   three single-use plastic management scenarios for Sichang Island using
   lifecycle assessment (LCA) with seven impact categories. The results of
   the LCA demonstrated that scenario S3 under the Sichang model, which
   includes refuse-derived fuel (75\%) and recycling (25\%), was the most
   environmentally friendly. In addition, the findings of this study
   indicate that sustainable management should separate waste at the source
   and promote the utilization of environmentally friendly materials.},
