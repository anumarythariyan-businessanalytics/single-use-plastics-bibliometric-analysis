@article{ WOS:001506095400019,
Author = {Biyada, Saloua and Urbonavicius, Jaunius},
Title = {Could bioplastics mitigate and solve world's plastic invasion?},
Journal = {JOURNAL OF ENVIRONMENTAL CHEMICAL ENGINEERING},
Year = {2025},
Volume = {13},
Number = {4},
Month = {AUG},
Abstract = {The widespread convenience of plastic items in daily life has
   progressively brought the downside of the plastic era to the fore. The
   predominant endurance and recalcitrant nature of plastics has prompted a
   considerable expansion in the proportion of plastics in the ecosystem,
   leading to the build-up of roughly 20 million tonnes of plastics in
   aquatic and terrestrial ecosystems yearly. The necessity of replacing
   plastic products has led to a flurry of research into durable
   alternatives to petroleum-based polymers. In this regard, bioplastics
   have been advocated as an alternative to conventional plastics for many
   purposes, such as single-use plastic bags, throwaway cutlery and
   crockery, and plastic films for mulching in agriculture. Nevertheless,
   doubts have emerged surrounding the environmental sustainability of
   bioplastics, notably concerning micro-and nano-plastic biodegradability
   and the emission of additives, all of which have a decisive impact on
   human health and environment. This study critically considers the
   state-of-the-art in the field based on a systematic literature search
   concerning the future challenges of applying biodegradable polymers as
   an alternative to conventional plastics and their sources, build-up,
   occurrence, hazardousness, environmental and public health impact, waste
   management options, together with bioremediation methods for
   bioplastics.},
